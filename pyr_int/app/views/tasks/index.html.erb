<p id="notice"><%= notice %></p>

<h1>Listing Tasks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Mqinfo</th>
      <th>Stdout rez</th>
      <th>Work status</th>
      <th>Errors suppz</th>
      <th>Manager log</th>
      <th>Output suppz</th>
      <th>Status suppz</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tasks.each do |task| %>
      <tr>
        <td><%= task.name %></td>
        <td><%= task.mqinfo %></td>
        <td><%= task.stdout_rez %></td>
        <td><%= task.work_status %></td>
        <td><%= task.errors_suppz %></td>
        <td><%= task.manager_log %></td>
        <td><%= task.output_suppz %></td>
        <td><%= task.status_suppz %></td>
        <td><%= link_to 'Show', task %></td>
        <td><%= link_to 'Edit', edit_task_path(task) %></td>
        <td><%= link_to 'Destroy', task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Task', new_task_path %>
